<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:vm="clr-namespace:UIPackageManager.ViewModels"
                    xmlns:vw="clr-namespace:UIPackageManager.Views">

	<DataTemplate DataType="{x:Type vm:InnovatorSettingsViewModel}">
		<vw:InnovatorSettingsView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:PackageListViewModel}">
		<vw:PackageListView></vw:PackageListView>
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:PackagePartSelectionViewModel}">
		<vw:PackagePartSelectionView></vw:PackagePartSelectionView>
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:RequiredPropertiesViewModel}">
		<vw:RequiredPropertiesView></vw:RequiredPropertiesView>
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:ProgresPageViewModel}">
		<vw:ProgresPageView></vw:ProgresPageView>
	</DataTemplate>

</ResourceDictionary>